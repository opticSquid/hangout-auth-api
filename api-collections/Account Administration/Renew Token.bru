meta {
  name: Renew Token
  type: http
  seq: 4
}

post {
  url: {{base_url}}/public/renew
  body: json
  auth: none
}

body:json {
  {
    "token": "{{refresh_token}}"
  }
}

script:post-response {
  bru.setEnvVar("access_token",res.body.accessToken);
  bru.setEnvVar("refresh_token",res.body.refreshToken);
}
